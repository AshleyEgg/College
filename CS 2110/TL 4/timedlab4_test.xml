<test-suite>
    <test-case>
        <name>caesar_encode("ABCDEFGHIJKLM", 0, 13, 1)</name>
        <has-max-executions>1</has-max-executions>
        <max-executions>1000000</max-executions>
        <randomize>1</randomize>
        <fully-randomize>1</fully-randomize>
        <input>
            <test-value>
                <address>STRING</address>
                <value>x7000</value>
            </test-value>
            <test-string>
                <address>STRING</address>
                <value>ABCDEFGHIJKLM</value>
            </test-string>
            <test-subr>
                <name>caesar_encode</name>
                <stack>xF000</stack>
                <r5>xCAFE</r5>
                <r7>x8000</r7>
                <params>x7000, 0, 13, 1</params>
            </test-subr>
        </input>
        <output>
            <test-string points="4">
                <address>STRING</address>
                <value>BCDEFGHIJKLMN</value>
            </test-string>
            <test-subr>
                <answer>13</answer>
                <calls>
                    <call><name>caesar_encode</name><params>x7000, 1, 13, 1</params></call>
                </calls>
                <points>
                    <answer>2</answer>
                    <calls>3</calls>
                    <r7>1</r7>
                    <r6>1</r6>
                    <r5>1</r5>
                    <params>2</params>
                    <read-answer>1</read-answer>
                </points>
                <deductions-per-mistake>1</deductions-per-mistake>
            </test-subr>
        </output>
    </test-case>
